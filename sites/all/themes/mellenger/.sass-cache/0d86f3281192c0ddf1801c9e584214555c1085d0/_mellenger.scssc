header, main, nav, footer{
  @extend .clearfix;
  position:relative;
  z-index:2;
}

header{
  padding-left: 5%;
  padding-right:5%;
  height:70px;
  background-color: $bright-blue;
  position:fixed;
  z-index:500;
  width:100%;
  @include box-shadow(rgba(#000, 0.25) 0 0.5px 25px -2px);
  > div{
    max-width:1028px;
    margin:0 auto;

    a.logo{
      margin-top:30px;
      display:block;
      width:141px;
      height:28px;
      float:left;
      background-image:url('img/logo.png');
      @include respond-to(handhelds){ width: (141px/4*3); height:(28px/4*3); margin-top:34px; }
      background-repeat:no-repeat;
      background-size: cover;
    }

    .block-menu{
      @extend .clearfix;
      float:right;
      h2{
        display:none;
      }
      ul.menu{
        float:right;
        margin-top: 24px;
        padding-left:0px;
        li{
          float:left;
          list-style-type:none;
          padding-left:0px;
          &:first-child{
            a{
              padding-left:0px;
            }
          }
          a{
            font-size: $menu-font;
            @include respond-to(handhelds){ font-size: ($menu-font/4*3); }
            color:#fff;
            padding-left:20px;
            font-family: "FranklinGothicFSMed";
            font-weight: 500;
          }
        }
      }
    }
  }
}

main{
  padding-top:70px;
  margin-bottom:215px;
  background-color:#fff;
}

footer{
  height:215px;
  width:100%;
  left:0px;
  bottom:0px;
  position:fixed;
  z-index:-1 !important;
  background-color: $bright-blue;
  > div{
    max-width:1028px;
    margin: $grid-gutter-width auto !important;
    #block-block-1, #block-block-2{
      margin-top: 165px;
    }
    li{
      &:first-child{
        a{
          border-left:none !important;
        }
      }
      a{
         padding-left:7px !important;
         margin-left:7px !important;
         border-left:1px solid #fff !important;
         margin-right:0px !important;
      }
    }
    p, a{
      font-size:$footer-normal;
      line-height: $footer-normal;
      color:#fff;
      float:left;
      padding:0px !important;
      margin-right:20px;
      font-weight:300;
    }
  }
}

section{
  @extend .clearfix;
  padding-left: 5%;
  padding-right:5%;
	h1.title{
    font-size: $large-header;
    line-height:$large-header;
    @include respond-to(handhelds){ font-size: ($large-header/5*3); line-height: ($large-header/5*3); }
    color: $bright-blue;
    margin-top:0px;
    margin-bottom:1em;
  }
  h2{
    font-size: $medium-header;
    line-height:$large-header;
    @include respond-to(handhelds){ font-size: ($medium-header/5*3); line-height: ($large-header/5*3); }
    color: $plain-text;

    position: relative;
    left: -300%;

    -webkit-transition: all 1s ease-in-out;
    -moz-transition: all 1s ease-in-out;
    -o-transition: all 1s ease-in-out;
    transition: all 1s ease-in-out;
  }
  h3{
    font-size: $small-header;
    line-height: $small-header;
    @include respond-to(handhelds){ font-size: ($small-header/4*3); line-height: ($small-header/4*3); }
    color: $plain-text;
    margin-bottom:35px;
  }
  p{
    font-size: $text;
    @include respond-to(handhelds){ font-size: ($text/4*3); }
    color: $plain-text;
    font-family: $font-family-serif;
    a{
      color: $bright-blue;
    }
    &.large{
      font-size: $large-text;
      @include respond-to(handhelds){ font-size: ($large-text/4*3); }
      font-family: "FranklinGothicFSBook";
    }
    &.client{
      border-bottom:1px solid $plain-text;
      padding-bottom:2px;
      display:inline-block;
    }
  }
  blockquote{
    font-size: $pullquote;
    line-height: $pullquote;
    @include respond-to(handhelds){ font-size: ($pullquote/2); line-height: ($pullquote/2); }
    color: $grey-blue;
    font-weight:500;
    margin:0;
    padding:0;
    &:before{
      content:"\201C";
    }
    &:after{
      content:"\201D";
    }
  }

  ul{
    padding-left:80px;
    &.menulike{
      padding-left:0px;
      li{
        font-family: $font-family-serif;
        font-size: $large-regular;
        border-bottom: 1px dashed #b3b3b3;
        padding-top:3px;
        padding-bottom:3px;
        &:before{
          display:none;
        }
      }
    }
    &.blacklist{
      padding-left: 40px;
      li{
        font-family: $font-family-serif;
        font-size: $large-regular;
        margin-bottom: 5px;
        &:before{
          width:5px;
          height:5px;
          background-color:#3d4244;
          left: -13px;
          top: 11px;
        }
      }
    }
    li{
      list-style-type:none;
      position:relative;
      font-family: "FranklinGothicFSBook";
      font-size: $unordered-list;
      @include respond-to(handhelds){ font-size: ($unordered-list/4*3); }
      &:before{
        content:"";
        display:block;
        width:9px;
        height:9px;
        background-color: $bright-blue;
        @include border-radius(9px);
        position:absolute;
        left:-20px;
        top:15px;
      }
    }
  }

  > div{
    max-width:1028px;
    margin: $grid-gutter-width auto !important;

    &.barbottom{
      border-bottom:1px solid $dark-grey;
      padding-bottom:25px;
      margin-bottom:10px !important;
      &.thickbar{
        border-bottom: 10px solid $dark-grey;
        margin-bottom:0px !important;
      }
    }
  }

  &.team{
    h4{
      margin-bottom: 4px;
    }
    h5{
      font-size: $med-large;
      color:#999999;
    }
    p{
      font-size: $large-regular;
    }
    .team-pic{
      text-align:center;
      margin-bottom:15px;
      img{
        @include border-radius(500px);
      }
    }
  }

  &.hero{
    position:relative;
    position: relative;
    overflow: hidden;
    width: 100%;
    margin-right: 0px;

    p.client{
      color:#fff;
      border-bottom: 1px solid #fff;
    }
    h1{
      color:#fff;
    }

    > div{
      padding-bottom: 15em;
      div.hero-img{
        position:absolute;
        top:0px;
        left:-5%;
        right:-5%;
        bottom:0px;
        overflow:hidden;
        z-index:-1;
        background-size: cover;
        background-repeat: no-repeat;
        background-position: 50% 50%;
      }
    }
  }

  &.purple, &.grey, &.greyblue, &.blue{
    h2{
      margin-top:190px;
      margin-bottom:190px;
    }
  }

  &.purple, &.grey{
    h1{
      color: $white;
    }
    h2{
      color: $white;
    }
    h3{
      color: $white;
    }
    p{
      color: $white;
      &.large{
        
      }
    }
    blockquote{
      color: $white;
    }
  }

  &.purple{
      background-color: $dark-purple;
    &.light{
      background-color: $light-purple;
    }
  }

  &.grey{
      background-color: $dark-grey;
    &.light{
      background-color: $light-grey;
      color: $plain-text;
    }
  }

  &.blue{
    h1{
      color: $white;
    }
    h2{
      color: $white;
    }
    h3{
      color: $white;
    }
    p{
      color: $white;
      &.large{
        
      }
    }
    blockquote{
      color: $white;
    }
  }

  &.greyblue{
      background-color: $grey-blue;

    &.light{
      background-color: $light-grey-blue;
      color: $plain-text !important;
    }
  }

  &.blue{
      background-color: $bright-blue;
    &.light{
      background-color: $light-blue;
    }
  }


}

















#lw3dwrapper{
  position: fixed;
  z-index: 1;
  left:0px;
  top:0px;
  width:100%;
  height:100%;
  -webkit-perspective: 000px;
  -webkit-perspective-origin: 50% 100px;

  -moz-perspective: 000px;
  -moz-perspective-origin: 50% 100px;

  -ms-perspective: 000px;
  -ms-perspective-origin: 50% 100px;

  perspective: 000px;
  perspective-origin: 50% 100px;
}

#livingwire-wrapper{
  width:344px;
  height:563px;
  position:absolute;
  cursor:pointer;

  -webkit-transform-style: preserve-3d;
  -webkit-animation: spin 5s infinite linear;
  
  -moz-transform-style: preserve-3d;
  -moz-animation: spin 5s infinite linear;

  -ms-transform-style: preserve-3d;
  -ms-animation: spin 5s infinite linear;

  transform-style: preserve-3d;
  animation: spin 5s infinite linear;

}

i{
  height:4px;
  width:1px;
  top:0px;
  left:0px;
  background-color:#f2f2f2;
  position:absolute;
  display:block;
  border-radius: 4px;
}

#interactive{ visibility: hidden !important; width:1px; }




